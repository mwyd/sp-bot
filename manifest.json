        "js": [
          "/lib/functions.js",
          "/lib/Vue.js",
          "/lib/Vuex.js",
          "/content/Item.js",
          "/content/Bot.js",
          "/content/Settings.js",
          "/content/SaleGuard.js",
          "/content/Home.js",
          "/content/Tab.js",
          "/store/modules/app.js",
          "/store/modules/bots.js",
          "/store/modules/saleguard.js",
          "/store/modules/session.js",
          "/store/modules/ws.js",
          "/store/store.js",
          "index.js"
        ],
        "css": [
          "/style/bot.css",
          "/style/home.css",
          "/style/saleguard.css",
          "/style/app.css",
          "/style/settings.css",
          "/style/tab.css"
        ],
